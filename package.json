{
  "name": "@ftajs/qjsc",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "license": "MIT",
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "napi": {
    "binaryName": [
      "qjsc",
      "qjsc3"
    ],
    "targets": [
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "x86_64-unknown-linux-gnu"
    ]
  },
  "scripts": {
    "install": "echo '@ftajs/qjsc installed'",
    "build": "node ./scripts/build.js",
    "artifacts": "node ./scripts/artifacts.js",
    "test": "ava __test__/*.mjs"
  },
  "dependencies": {
    "commander": "^8.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@ftajs/cli": "^0.0.1",
    "ava": "^5.1.1",
    "lodash": "^4.14.202",
    "node-addon-api": "^7.0.0"
  }
}
